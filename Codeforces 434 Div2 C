#include <bits/stdc++.h>
using namespace std;

int main()
{
    string s;
    cin>>s;
    int flag=0;
    int len=s.length();
    for(int i=0;i<len-2;i++)
    {
        if(s.at(i)!='a'&&s.at(i)!='e'&&s.at(i)!='i'&&s.at(i)!='o'&&s.at(i)!='u')
        {
            if(s.at(i+1)!='a'&&s.at(i+1)!='e'&&s.at(i+1)!='i'&&s.at(i+1)!='o'&&s.at(i+1)!='u')
            {
                if(s.at(i+2)!='a'&&s.at(i+2)!='e'&&s.at(i+2)!='i'&&s.at(i+2)!='o'&&s.at(i+2)!='u')
                {
                    if((s.at(i)==s.at(i+1)&&s.at(i+1)==s.at(i+2)))
                    {
                        flag=0;
                    }
                    else
                    {
                        flag=1;
                        int newl=len+1;
                        for(int j=i+2;j<newl;j++)
                        {
                            char ch=s.at(j);
                            s.at(j)=' ';
                            s.at(j+1)=ch;
                        }
                    }
                }
            }
        }    
    }
    cout<<s;
    //cout << "Hello World!" << endl;
    return 0;
}
